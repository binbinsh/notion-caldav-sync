name = "notion-caldav-sync"
main = "src/app/worker.py"
compatibility_date = "2025-11-09"
compatibility_flags = ["python_workers"]

[[kv_namespaces]]
binding = "STATE"
id = "${CLOUDFLARE_STATE_NAMESPACE}"

[vars]
STATUS_EMOJI_STYLE = "symbol"

[triggers]
crons = ["*/5 * * * *"]

[observability]
[observability.logs]
enabled = true
head_sampling_rate = 1
invocation_logs = true
persist = true